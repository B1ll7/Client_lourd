<Window x:Class="ClientLourdWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientLourdWpf"
        mc:Ignorable="d"
        Title="Agenda LaManu" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Menu>
                <MenuItem Header="Clients" Height="20" Width="100">
                    <MenuItem Header="Ajout Client" Click="MenuItem_Click" />
                    <MenuItem Header="Liste Clients" Click="MenuItem_Click_1" />
                </MenuItem>
                <MenuItem Header="Courtier" Height="20" Width="100">
                    <MenuItem Header="Ajout Coutier" Click="MenuItem_Broker_Click" />
                    <MenuItem Header="Liste Courtier" Click="MenuItem_Broker_Click_1" />
                </MenuItem>
            </Menu>
        </StackPanel>
        <Frame x:Name="main"></Frame>
    </Grid>
</Window>
